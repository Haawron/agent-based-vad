{
    "exclude": [
        "__pycache__",
        "*.egg-info/",
        "*.gz",
        "data/",
        "logs/",
        "output/",
        "output copy/",
        "datasets/",
    ],
    "extraPaths": [
        "/code/libs/imagebind",
        "/code/libs/languagebind",
        "/code/libs/internvideo/InternVideo2/multi_modality",
        "/code/libs/perception_models",
    ],
    "reportPrivateImportUsage": false,  // pytorch is not familiar with the private import
    "reportCallIssue": "none",  // pyright doesn't consider C-based functions
    "reportAssignmentType": "none",  // pyright doesn't consider Optional types
    "reportArgumentType": "none",  // pyright doesn't consider Optional types
    "reportIncompatibleMethodOverride": "none",  // pyright doesn't consider Optional types
    "reportRedeclaration": "none",  // pyright doesn't consider Optional types
    "reportAttributeAccessIssue": "none",  // pyright doesn't consider Optional types
    "reportOptionalOperand": "none",  // pyright doesn't consider Optional types
    "reportOptionalMemberAccess": "none",  // pyright doesn't consider Optional types
    "typeCheckingMode": "off",
}
